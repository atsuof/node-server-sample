{
  "name": "node-server-sample",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npx tsc && npx cpx \"config/**\" dist/config",
    "copy": "npx cpx \"config/**\" dist/config",
    "test": "NODE_ENV=test jest --coverage --colors --verbose",
    "start": "NODE_ENV=development node ./dist/server.js",
    "startproduction": "NODE_ENV=production node ./dist/server.js",
    "tsnode": "NODE_ENV=development ts-node ./src/js/server.ts",
    "tsdev": "NODE_ENV=development  ts-node-dev --respawn ./src/js/server.ts",
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/config": "0.0.34",
    "@types/crypto-js": "^3.1.43",
    "@types/jszip": "^3.1.7",
    "@types/ldapjs": "0.0.1",
    "@types/log4js": "^2.3.5",
    "@types/mongoose": "^5.5.35",
    "@types/natural": "^0.6.3",
    "assert": "^2.0.0",
    "axios": "^0.19.0",
    "config": "^3.2.4",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.19",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "fast-levenshtein": "^2.0.6",
    "jaro-winkler": "^0.2.8",
    "jszip": "^3.2.2",
    "ldapjs": "^1.0.2",
    "log4js": "^5.3.0",
    "mongoose": "^5.8.3",
    "natural": "^0.6.3",
    "xlsx": "^0.15.4",
    "mem": ">=4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.22",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "jest-jenkins-reporter": "^1.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleNameMapper": {
      "^#/(.+)": "./src/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "testResultsProcessor": "jest-jenkins-reporter",
    "testEnvironment": "node"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "report.xml",
    "indent": 4
  }
}
